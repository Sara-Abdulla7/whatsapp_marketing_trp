<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron that processes the whatsapp marketing queue -->
        <record id="ir_cron_whatsapp_marketing_queue" model="ir.cron">
            <field name="name">Whatsapp Marketing: Process queue</field>
            <field name="model_id" ref="model_whatsapp_marketing"/>
            <field name="state">code</field>
            <field name="code">model._process_whatsapp_marketing_queue()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>
    </data>
</odoo>
